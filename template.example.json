{
   "AWSTemplateFormatVersion": "2010-09-09",
   "Description": "A serverless application to plot RI Utilization data point to a custom CloudWatch Metrics.",
   "Resources": {
      "RIUtilizationPlotter": {
         "Properties": {
            "CodeUri": "./package/lambda-ri-utilization-plotter.zip",
            "FunctionName": "ri-utilization-plotter",
            "Handler": "lambda-ri-utilization-plotter",
            "MemorySize": 128,
            "Role": "WRITE YOUR LAMBDA EXECUTION ROLE",
            "Runtime": "provided",
            "Timeout": 30,
            "Environment": {
              "Variables": {
                "RUST_LOG": "info",
                "RUST_BACKTRACE": "1"
              }
            }
         },
         "Type": "AWS::Serverless::Function"
      }
   },
   "Transform": "AWS::Serverless-2016-10-31"
}
